<div class="carrito">
    <table class="table">
  <thead>
    <tr>
      <th scope="col">numero de carrito:</th>
      <th scope="col">productos elegidos:</th>


    </tr>
  </thead>
  <tbody>

    {{#each carrito}}
        <tr>
            <th scope="row">{{this.id}}</th>
            {{#each this.productos}}
              <td><img width="50" src="{{this.foto}}"  alt="not found"></td>              
            {{/each}}
        </tr>
    {{/each}}
  </tbody>
</table>

<button class="btn btn-primary" onclick="location.href='/enviarMensajes'" >finalizar compra</button>

</div>

<style>
  .carrito{
    border: black solid;
    margin: 50px 20px 50px 20px;
  }
</style>