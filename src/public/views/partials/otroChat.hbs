<div class="otroChat">
    {{#each data}}
        <div class="card">
            <div class="card-header">
                {{this.email}}
            </div>
            <div class="card-body">
                <blockquote class="blockquote mb-0">
                    <p>{{this.mensaje}}</p>
                </blockquote>
            </div>
        </div>
    {{/each}}
</div>